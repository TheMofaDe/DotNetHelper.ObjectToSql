{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net452"
      }
    }
  ],
  "build": {

    "content": [
      {
        "files": [ "*.yml","*.md" ],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [ "*.md","*.yml" ],
        "src": ".",
        "dest": "."
      },

      {
        "files": [ "*.md", "*.yml" ],
        "src": "./tutorials",
        "dest": "./tutorials"
      }
    ],
    "resource": [
      {
        "files": [ "images/**", "langwordmapping/**" ],
        "exclude": [ "**.meta" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotNetHelper.ObjectToSql",
      "_gitContribute": {
        "repo": "https://github.com/TheMofaDe/DotNetHelper.ObjectToSql",
        "branch": "master",
        "apiSpecFolder": "docs-ref-overwrite"
      },
      "_disableContribution": false,
      "_disableBreadcrumb": false,
      "_appFaviconPath": "logo/favicon.ico",
      "_disableNavbar": false,
      "_enableSearch": true,
       "_navRel": "toc.html",
      //    "_appLogoPath": "logo/lucene-net-color.png",
      "_appFooter": "Copyright Â© 2019 Joseph McNeal Jr"
    },
    "dest": "./",
    "globalMetadataFiles": [],
    "template": [ "statictoc"],
    // "template": [ "default", "templates/darkfx" ],
    // "template": [ "statictoc", "templates/material" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "keepFileLink": true,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}
